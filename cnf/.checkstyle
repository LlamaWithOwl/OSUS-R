<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="all" location="checkstyle/checkstyle-rules-all-files.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="all" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
    <file-match-pattern match-pattern=".classpath|.project|.checkstyle|.jar|.settings|.DS_Store" include-pattern="false"/>
    <file-match-pattern match-pattern="gradle/template/." include-pattern="false"/>
    <file-match-pattern match-pattern="gradle/doc/." include-pattern="false"/>
    <file-match-pattern match-pattern="build-template.xml" include-pattern="false"/>
    <file-match-pattern match-pattern="tmp.build.properties" include-pattern="false"/>
    <file-match-pattern match-pattern="custom.bnd" include-pattern="false"/>
    <file-match-pattern match-pattern="index.xml.gz.*" include-pattern="false"/>
    <file-match-pattern match-pattern="cache" include-pattern="false"/>
    <file-match-pattern match-pattern="javadoc" include-pattern="false"/>
    <file-match-pattern match-pattern="jpm-index.json" include-pattern="false"/>
  </fileset>
  <filter name="NonSrcDirs" enabled="false"/>
</fileset-config>
